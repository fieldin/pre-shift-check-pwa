{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/icons/*",
          "/**/*.svg",
          "/**/*.png",
          "/**/*.jpg",
          "/**/*.jpeg",
          "/**/*.woff2"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-assets",
      "urls": [
        "/api/assets",
        "/api/assets/*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "10s"
      }
    },
    {
      "name": "api-checklists",
      "urls": [
        "/api/checklists",
        "/api/checklists/*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "1d",
        "timeout": "10s"
      }
    },
    {
      "name": "api-faults",
      "urls": [
        "/api/faults",
        "/api/faults/*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "1h",
        "timeout": "10s"
      }
    },
    {
      "name": "api-write",
      "urls": [
        "/api/events",
        "/api/events/*",
        "/api/faults"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 0,
        "maxAge": "0u",
        "timeout": "30s"
      }
    }
  ]
}
